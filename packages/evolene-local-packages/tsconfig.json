{
  "compilerOptions": {
   /* Visit https://aka.ms/tsconfig to read more about this file */
   /* Language and Environment */
    "target": "es2020",
    /* Modules */
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    /* Completeness */
    "skipLibCheck": true
  }
}
