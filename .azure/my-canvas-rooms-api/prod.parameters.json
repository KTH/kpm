{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "my-canvas-rooms-api-prod"
    },
    "secretNames": {
      "value": []
    },
    "environmentVariables": {
      "value": {
        "API_VERSION": "~1.38.0",
        "CANVAS_API_URL": "https://canvas.kth.se/api/v1",
        "NODE_ENV": "production",
        "PROXY_PATH_PREFIX": "/api/my-canvas-rooms"
      }
    },
    "monitorPath": {
      "value": "/api/my-canvas-rooms/_monitor"
    },
    "websitePort": {
      "value": "3000"
    }
  }
}
