{
  "name": "kpm",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "concurrently -n server,parcel -k \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch server app.js | bunyan -o short",
    "dev:client": "parcel watch client/kpm.js",
    "format": "prettier --write .",
    "start": "node app.js",
    "test:lint": "prettier --check .",
    "test": "npm run test:lint",
    "clean": "rm -rf ./dist",
    "build:kth-style": "mkdir -p ./dist/kth-style && cp -r ./node_modules/kth-style/dist/* ./dist/kth-style",
    "build:client": "parcel build client/kpm.js",
    "build": "npm run clean && npm run build:kth-style && npm run build:client"
  },
  "keywords": [],
  "author": "KTH Royal Institute of Technology",
  "license": "MIT",
  "dependencies": {
    "@kth/reqvars": "^2.0.1",
    "bunyan": "^1.8.14",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fast-xml-parser": "^3.17.4",
    "got": "^11.8.0",
    "handlebars": "^4.7.6",
    "kth-style": "^6.0.1",
    "skog": "^1.0.1-alpha.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2"
  }
}
